{% extends 'base.html.twig' %}

{% block title %}All Songs{% endblock %}

{% block body %}
<h1>All Songs</h1>
<ul class="table">
    {% for song in songs %}
        <li>
            <a href="{{ path('song.show', {id: song.id, slug: song.slug})}}">{{ song.title }}</a>
        </li>
    {% endfor %}
</ul>
{% endblock %}
